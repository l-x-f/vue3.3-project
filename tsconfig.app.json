{
  "extends": "@vue/tsconfig/tsconfig.json",
  "include": ["types", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "composite": true,
    "baseUrl": ".",
    "types": ["vite/clict"],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
